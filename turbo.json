{
  "pipeline": {
    "build": {
      "outputs": [
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "shared-build": {
      "dependsOn": ["@elune/shared#build"]
    },
    "kucom-dev": {
      "dependsOn": ["shared-dev","@elune/kucomponent#dev"]
    },
    "kucom-build": {
      "dependsOn": ["@elune/kucomponent#build"]
    },

    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
